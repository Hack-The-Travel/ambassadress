<?xml version="1.0" encoding="utf-8" ?>
<request>
    <security>
        <token value="{{ security_token }}"/>
    </security>
    <message type="sms">
        <sender>REDSMS.RU</sender>
        <text>{{ message }}</text>
        {% for abonent in to %}
        <abonent phone="{{ abonent }}" number_sms="{{ loop.index }}" />
        {% endfor %}
    </message>
</request>